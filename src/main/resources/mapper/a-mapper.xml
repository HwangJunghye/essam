<?xml version="1.0" encoding="UTF-8"?>
<!-- mapper(sql문) 작성 -->
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.essam.www.a.IADao">


	<select id="getStudentList" resultType="">
		SELECT * FROM CLASS WHERE MBID=#{mbId}
	</select>
	
	<select id="getStudentInfo" resultType="">
		SELECT * FROM MEMBER WHERE MBID=#{mbId}
	</select>

</mapper>