<?xml version="1.0" encoding="UTF-8"?>
<!-- mapper(sql문) 작성 -->
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.essam.www.e.IEDao">
	<update id="changePassword">
		UPDATE MEMBER 
		SET MBPWD = #{mbPwd} 
		WHERE MBID = #{mbId}
	</update>
</mapper>